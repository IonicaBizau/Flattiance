#!/usr/bin/make -f
# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@ --with scour --parallel

override_dh_install:
	dh_install --fail-missing
	# We ship pre-built copies of the user-desktop icon
	for p in debian/ubuntu-mono/usr/share/icons/ubuntu-mono-* ; do cp -avt $$p places ; done

